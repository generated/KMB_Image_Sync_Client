    <wsdl:operation name="GetAssetsMetadata">
      <wsdl:documentation>&lt;summary&gt;
            &lt;para&gt;Get a list of &lt;see cref="T:DataContract.Asset.AssetFieldValues" /&gt;&lt;/para&gt;
